{
    "manifest_version": 3,
    "name": "Stopper Popup",
    "description": "A popup that stops you",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://www.youtube.com/*",
          "https://linear.app/*"
        ]
      }
    ]
}